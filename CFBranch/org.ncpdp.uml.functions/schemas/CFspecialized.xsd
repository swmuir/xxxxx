      <xsd:element minOccurs="0" name="ShippingAmount" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Amount of shipping cost that was applied to the prescription.  Include decimal and sign</xsd:documentation>
        </xsd:annotation>
