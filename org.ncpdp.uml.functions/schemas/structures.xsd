  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Diagnosis  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Diagnosis">
    <xsd:sequence>
      <xsd:element name="ClinicalInformationQualifier" type="ecl:DiagnosisClinicalInformationQualifierCode"/>
      <xsd:element name="Primary" type="structures:PrimaryDiagnosis">
        <xsd:annotation>
          <xsd:documentation>The prescriber supplied or pharmacy inferred code for the diagnosis.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Secondary" type="structures:SecondaryDiagnosis">
        <xsd:annotation>
          <xsd:documentation>The prescriber supplied or pharmacy inferred code for the secondary diagnosis.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DiagnosisGeneral  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DiagnosisGeneral">
    <xsd:annotation>
      <xsd:documentation>Element may be used only in the long-term care setting, where there are established processes supporting the sharing of diagnosis information between a resident’s facility and pharmacy. Note: The medication information enables communication of prescription-specific diagnosis information only—appropriate for use in new prescription messaging, but insufficient to support management of a resident’s full drug regimen. The intent is to convey the diagnosis that the prescribed medication is for.  There is also a need to convey diagnosis information in the Census event.  The difference here is that there is no related drug.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="SourceOfInformation" type="ecl:SourceOfInformationCode">
        <xsd:annotation>
          <xsd:documentation>Sender indicates where the sender received the diagnosis information if known. 
The source of information is the ultimate source. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>When this particular diagnosis was determined for the patient and when it was no longer active – if known. Value 07 Effective Date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>When this particular diagnosis was determined for the patient and when it was no longer active – if known. Value 36 Expiration Date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProblemType" type="datatypes:SNOMEDCode">
        <xsd:annotation>
          <xsd:documentation>Describes the type of problem that exists for this patient. Value LD</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ProblemNameCoded" type="datatypes:ProblemNameCode">
        <xsd:annotation>
          <xsd:documentation>Identifies the Diagnosis for this patient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
