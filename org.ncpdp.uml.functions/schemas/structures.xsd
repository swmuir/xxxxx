  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SIGType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SIGType">
    <xsd:annotation>
      <xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RepeatingSIG">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="SigSequencePositionNumber" type="datatypes:n..2"/>
            <xsd:element minOccurs="0" name="MultipleSigModifier" type="datatypes:ANDORTHEN"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CodeSystem">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="SNOMEDVersion" type="datatypes:an..14"/>
            <xsd:element minOccurs="0" name="FMTVersion" type="datatypes:an..14"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FreeText">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="SigFreeTextStringIndicator">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="1|2|3"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="SigFreeText" type="datatypes:an..140M"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Dose">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="DoseCompositeIndicator">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="1|2|3|4"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodModifierText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodModifierCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodModifierCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DoseQuantity" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="DoseFormText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="DoseFormCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DoseFormCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DoseRangeModifier" type="datatypes:TOOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="DoseCalculation">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="DosingBasisNumericValue" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="DosingBasisUnitOfMeasureText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="DosingBasisUnitOfMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DosingBasisUnitOfMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="BodyMetricQualifier">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="1|2"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="BodyMetricValue" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="CalculatedDoseNumeric" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="CalculatedDoseUnitOfMeasureText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="CalculatedDoseUnitOfMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="CalculatedDoseUnitOfMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DosingBasisRangeModifier" type="datatypes:TOOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Vehicle">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="VehicleName" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="VehicleNameCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="VehicleNameCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="VehicleQuantity" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="VehicleUnitOfMeasureText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="VehicleUnitOfMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="VehicleUnitOfMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="MultipleVehicleModifier" type="datatypes:ANDOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="RouteOfAdministration" type="structures:RouteOfAdministration"/>
      <xsd:element minOccurs="0" name="SiteOfAdministration">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="SiteOfAdministrationText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="SiteOfAdministrationCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="SiteOfAdministrationCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="MultipleAdministrationTimingModifier" type="datatypes:ANDORNOT"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Timing">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="AdministrationTimingText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="AdministrationTimingCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="AdministrationTimingCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="MultipleAdministrationTimingModifier" type="datatypes:ANDOR"/>
            <xsd:element minOccurs="0" name="RateOfAdministration" type="datatypes:an..11"/>
            <xsd:element minOccurs="0" name="RateUnitOfMeasureText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="RateUnitOfMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="RateUnitOfMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="TimePeriodBasisText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="TimePeriodBasisCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="TimePeriodBasisCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="FrequencyNumericValue" type="datatypes:an..11"/>
            <xsd:element minOccurs="0" name="FrequencyUnitsText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="FrequencyUnitsCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="FrequencyUnitsCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="VariableFrequencyModifier" type="datatypes:ANDORTO"/>
            <xsd:element minOccurs="0" name="IntervalNumericValue" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="IntervalUnitsText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="IntervalUnitsCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="IntervalUnitsCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="VariableIntervalModifier" type="datatypes:ANDORTO"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Duration">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="DurationNumericValue" type="datatypes:an..11M"/>
            <xsd:element name="DurationText" type="datatypes:an..50M"/>
            <xsd:element name="DurationTextCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="DurationTextCode" type="datatypes:an..15M"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaximumDoseRestriction">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="MaximumDoseRestrictionNumericValue" type="datatypes:n..18M"/>
            <xsd:element name="MaximumDoseRestrictionUnitsText" type="datatypes:an..50M"/>
            <xsd:element name="MaximumDoseRestrictionCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="MaximumDoseRestrictionUnitsCode" type="datatypes:an..15M"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableNumericValue" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableUnitsText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableUnitsCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableUnitsCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableDurationModifier" type="datatypes:ANDOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Indication">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="IndicationPrecursorText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="IndicationPrecursorCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="IndicationPrecursorCode" type="datatypes:an..15M"/>
            <xsd:element minOccurs="0" name="IndicationText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="IndicationTextCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="IndicationTextCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="IndicationValueText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="IndicationValueUnit" type="datatypes:n..18"/>
            <xsd:element minOccurs="0" name="IndicationValueUnitOfMeasureText" type="datatypes:an..50"/>
            <xsd:element minOccurs="0" name="IndicationValueUnitOfMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="IndicationValueUnitOfMeasureCode" type="datatypes:an..15M"/>
            <xsd:element minOccurs="0" name="IndicationVariableModifier" type="datatypes:ANDORTO"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Stop">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="StopIndicator">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="Y|N"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SecondaryDiagnosis  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SecondaryDiagnosis">
    <xsd:sequence>
      <xsd:element name="Qualifier" type="ecl:SecondaryDiagnosisCodeQualifierCode"/>
      <xsd:element name="Value" type="ecl:Code"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Supervisor  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Supervisor">
    <xsd:annotation>
      <xsd:documentation>To describe the supervisor. Supervisor information may be needed based upon state law. A supervisor is the supervising physician under whose authority the prescription is being prescribed by the prescriber. 

Value SU</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:SupervisorOptional">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:ProviderID">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ClinicName" type="datatypes:an..35">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorMandatoryAddress  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorMandatoryAddress">
    <xsd:annotation>
      <xsd:documentation>Value SU</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:Supervisor">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:ProviderID">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ClinicName" type="datatypes:an..35">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorOptional  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorOptional">
    <xsd:annotation>
      <xsd:documentation>To describe the supervisor. Supervisor information may be needed based upon state law. A supervisor is the supervising physician under whose authority the prescription is being prescribed by the prescriber. </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Identification" type="datatypes:ProviderID">
        <xsd:annotation>
          <xsd:documentation>On the mandatory transactions: One occurrence is the prescriber number. One occurrence may be used to identify the clinic. One occurrence may be used to identify a supervisor.
At least one occurrence must contain “HPI” (NPI). Prescriber individual (not organizational) NPI Number is mandatory. This is mandatory for the Prescriber (PC), not the Supervisor (SU). 
For all transactions except RxHistoryResponse, if the prescriber has an NPI, one occurrence must contain the value “HPI” (NPI). 
For all transactions except RxHistoryResponse, if the prescriber has a DEA number, one occurrence must contain the value “DH” (DEA Number).
				</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Specialty" type="datatypes:an..10"/>
      <xsd:element name="Name" type="datatypes:NameType"/>
      <xsd:element minOccurs="0" name="ClinicName" type="datatypes:an..35"/>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:annotation>
    <xsd:documentation>The NCPDP structures.xsd defines common and resuable domain structures typically composed of datatypes and 
potentially other structures.</xsd:documentation>
  </xsd:annotation>
</xsd:schema>