  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  CompoundIngredient  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="CompoundIngredient">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CompoundIngredient">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CompoundIngredientItemDescription" type="datatypes:an..105M"/>
            <xsd:element minOccurs="0" name="ItemNumber" type="datatypes:ProductCode"/>
            <xsd:element minOccurs="0" name="Strength" type="datatypes:an..70"/>
            <xsd:element minOccurs="0" name="StrengthForm" type="datatypes:NCICode">
              <xsd:annotation>
                <xsd:documentation>NCPDP Drug StrengthForm Terminology - available at 
http://www.cancer.gov/cancertopics/terminologyresources/page7
See External Code List Introduction for information on NCI Thesaurus Code Lists.

</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="StrengthCode" type="datatypes:NCICode"/>
            <xsd:element minOccurs="0" name="DEASchedule" type="datatypes:NCICode"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Quantity" type="datatypes:Quantity"/>
      <xsd:element maxOccurs="5" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
