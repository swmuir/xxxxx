  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SIGType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SIGType">
    <xsd:sequence>
      <xsd:element name="RepeatingSIG">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="SigSequencePositionNumber"/>
            <xsd:element minOccurs="0" name="MultipleSigModifier" type="datatypes:ANDORTHEN"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CodeSystem">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="SNOMEDVersion"/>
            <xsd:element minOccurs="0" name="FMTVersion">
              <xsd:simpleType>
                <xsd:restriction base="datatypes:an">
                  <xsd:maxLength value="14"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FreeText">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="SigFreeTextStringIndicator">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="1|2|3"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="SigFreeText"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="Dose">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="DoseCompositeIndicator">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="1|2|3|4"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
