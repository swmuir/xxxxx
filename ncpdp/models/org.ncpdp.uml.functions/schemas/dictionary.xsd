        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="SiteofAdministrationText"/>
            <xsd:element minOccurs="0" name="SiteofAdministrationCodeQualifier" type="datatypes:TerminologyType"/>
