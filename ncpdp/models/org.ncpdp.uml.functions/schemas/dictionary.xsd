            <xsd:element minOccurs="0" name="DoseDeliveryMethodCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodModifierText"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodModifierCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DoseDeliveryMethodModifierCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DoseQuantity"/>
            <xsd:element minOccurs="0" name="DoseFormText"/>
            <xsd:element minOccurs="0" name="DoseFormCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DoseFormCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DoseRangeModifier" type="datatypes:TOOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="DoseCalculation">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="DosingBasisNumericValue"/>
            <xsd:element minOccurs="0" name="DosingBasisUnitofMeasureText"/>
            <xsd:element minOccurs="0" name="DosingBasisUnitofMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="DosingBasisUnitofMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="BodyMetricQualifier">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="1|2"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="BodyMetricValue"/>
            <xsd:element minOccurs="0" name="CalculatedDoseNumeric"/>
            <xsd:element minOccurs="0" name="CalculatedDoseUnitofMeasureText"/>
            <xsd:element minOccurs="0" name="CalculatedDoseUnitofMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="CalculatedDoseUnitofMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="DosingBasisRangeModifier" type="datatypes:TOOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Vehicle">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="VehicleName"/>
            <xsd:element minOccurs="0" name="VehicleNameCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="VehicleNameCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="VehicleQuantity"/>
            <xsd:element minOccurs="0" name="VehicleUnitOfMeasureText"/>
            <xsd:element minOccurs="0" name="VehicleUnitOfMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="VehicleUnitOfMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="MultipleVehicleModifier" type="datatypes:ANDOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="RouteofAdministration"/>
