            <xsd:element minOccurs="0" name="AdministrationTimingCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="MultipleAdministrationTimingModifier" type="datatypes:ANDOR"/>
            <xsd:element minOccurs="0" name="RateofAdministration"/>
            <xsd:element minOccurs="0" name="RateUnitofMeasureText"/>
            <xsd:element minOccurs="0" name="RateUnitofMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="RateUnitofMeasureCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="TimePeriodBasisText"/>
            <xsd:element minOccurs="0" name="TimePeriodBasisCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="TimePeriodBasisCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="FrequencyNumericValue">
              <xsd:simpleType>
                <xsd:restriction base="datatypes:an">
                  <xsd:maxLength value="11"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="FrequencyUnitsText"/>
            <xsd:element minOccurs="0" name="FrequencyUnitsCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="FrequencyUnitsCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="VariableFrequencyModifier" type="datatypes:ANDORTO"/>
            <xsd:element minOccurs="0" name="IntervalNumericValue"/>
            <xsd:element minOccurs="0" name="IntervalUnitsText"/>
            <xsd:element minOccurs="0" name="IntervalUnitsCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="IntervalUnitsCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="VariableIntervalModifier" type="datatypes:ANDORTO"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Duration">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="DurationNumericValue">
              <xsd:simpleType>
                <xsd:restriction base="datatypes:anM">
                  <xsd:maxLength value="11"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="DurationText"/>
            <xsd:element name="DurationTextCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="DurationTextCode"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaximumDoseRestriction">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="MaximumDoseRestrictionNumericValue">
              <xsd:simpleType>
                <xsd:restriction base="datatypes:nM">
                  <xsd:maxLength value="18"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="MaximumDoseRestrictionUnitsText">
              <xsd:simpleType>
                <xsd:restriction base="datatypes:anM">
                  <xsd:maxLength value="50"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="MaximumDoseRestrictionCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="MaximumDoseRestrictionUnitsCode"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableNumericValue"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableUnitsText"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableUnitsCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableUnitsCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="MaximumDoseRestrictionVariableDurationModifier" type="datatypes:ANDOR"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Indication">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="IndicationPrecursorText"/>
            <xsd:element minOccurs="0" name="IndicationPrecursorCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="IndicationPrecursorCode"/>
            <xsd:element minOccurs="0" name="IndicationText"/>
            <xsd:element minOccurs="0" name="IndicationTextCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element minOccurs="0" name="IndicationTextCode" type="datatypes:an..15"/>
            <xsd:element minOccurs="0" name="IndicationValueText"/>
            <xsd:element minOccurs="0" name="IndicationValueUnit">
              <xsd:simpleType>
                <xsd:restriction base="datatypes:n">
                  <xsd:maxLength value="18"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="IndicationValueUnitofMeasureText"/>
            <xsd:element minOccurs="0" name="IndicationValueUnitofMeasureCodeQualifier" type="datatypes:TerminologyType"/>
            <xsd:element name="IndicationValueUnitofMeasureCode">
              <xsd:simpleType>
                <xsd:restriction base="datatypes:anM">
                  <xsd:maxLength value="15"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element minOccurs="0" name="IndicationVariableModifier" type="datatypes:ANDORTO"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Stop">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="StopIndicator">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="Y|N"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
